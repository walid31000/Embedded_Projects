#ifndef SSD_INTERFACE_H
#define SSD_INTERFACE_H


typedef enum
{
	ANODE,
	CATHODE
	
}Type_t;

typedef enum
{
	DIO_PORTA,DIO_PORTB,DIO_PORTC,DIO_PORTD
}Dio_Port_t;

typedef enum
{
	DIO_PORTA,DIO_PORTB,DIO_PORTC,DIO_PORTD
}Dio_Enable_Port;

typedef enum
{
	DIO_PIN0,
	DIO_PIN1,
	DIO_PIN2,
	DIO_PIN3,
	DIO_PIN4,
	DIO_PIN5,
	DIO_PIN6,
	DIO_PIN7
}Enable_Pin_t;

typedef struct
{
	Type_t;
	Dio_Port_t;
	Dio_Enable_Port;
	Enable_Pin_t;
	
}SSD_Config_t;








uint8 SSD_u8SetNumber(const SSD_Config_t* copy_SSD_Config,uint8 copy_u8Number);



#endif