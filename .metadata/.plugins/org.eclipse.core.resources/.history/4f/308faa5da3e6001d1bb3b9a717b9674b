#include <util/delay.h>
#include <avr/io.h>
#include "BIT_MATH.h"
#include "STD_TYPES.h"

void main(void)
{
	DDRC=0xFF;
uint8 SSD_ARR[10]={0b00111111,0b00000110,0b01011011,0b01001111,0b01100110,0b01101101,0b01111101,0b00000111,0b01111111,0b01101111};

	//SET_BIT(DDRA,0);
  uint8 i;
	while(1)
	{
	   for(i=0;i<=9;i++)
	   {
		PORTC=SSD_ARR[i];
	    _delay_ms(300);
	   }


	}
}

