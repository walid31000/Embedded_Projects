#include "STD_TYPES.h"
#include<util/delay.h>
#include "Err_Type.h"
#include "DIO_interface.h"

void main(void)
{
	/*set C0 as output*/
	DIO_u8SetPinDirection(DIO_PORTC,DIO_PIN0,DIO_PIN_OUTPUT);
	while(1)
	{
		DIO_u8SetPinValue(DIO_PORTC,DIO_PIN0,DIO_PIN_HIGH);
		_delay_ms(500);
		DIO_u8SetPinValue(DIO_PORTC,DIO_PIN0,DIO_PIN_LOW);
				_delay_ms(500);
	}




}
