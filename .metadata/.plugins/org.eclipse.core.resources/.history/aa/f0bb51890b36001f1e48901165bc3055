#include "STD_TYPES.h"
#include "BIT_MATH.h"
#include "Err_Type.h"
#include "DIO_interface.h"
#include "PORT_interface.h"
#include "DAC_interface.h"
#include "DAC_CFG.h"

void main (void)
{
	sint16 Local_s16Volt;
	PORT_voidIint();
	while(1)
	{
		for(Local_s16Volt=0;Local_s16Volt<5000;Local_s16Volt++)
		{
			DAC_voidSetAnalogVolt(Local_s16Volt );
		}

		for(Local_s16Volt=5000;Local_s16Volt>=0;Local_s16Volt--)
		{
			DAC_voidSetAnalogVolt(Local_s16Volt );
		}

	}



}
