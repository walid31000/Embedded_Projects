#include "STD_TYPES.h"
#include<util/delay.h>
#include "Err_Type.h"
#include "DIO_interface.h"

void main(void)
{
	/*set PORTC as output*/
	DIO_u8SetPortDirection(DIO_PORTC,0xff);
	while(1)
	{
		DIO_u8SetPortValue(DIO_PORTC,0x0f);
		_delay_ms(500);
		DIO_u8SetPortValue(DIO_PORTC,0xf0);
				_delay_ms(500);
	}




}
