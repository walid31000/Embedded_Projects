#include "STD_TYPES.h"
#include "BIT_MATH.h"
#include "Err_Type.h"
#include <util/delay.h>
#include "DIO_interface.h"
#include "LED_interface.h"
#include "LED_prv.h"

void main()
{
	DIO_u8SetPinDirection(DIO_PORTA,DIO_PIN0,DIO_PIN_OUTPUT);
	while(1)
	{
	Led_config_t LED1={DIO_PORTA,DIO_PIN0,ACT_HIGH};

	LED_u8SetON(&LED1 );
	_delay_ms(500);
	 LED_u8SetOFF(&LED1);
	 _delay_ms(500);
	}
}
